<%passages.forEach(function(passage) { %>
    <%- include('passage', {passage: passage, sub: true, subPassage:false}) %>
<%});%>
<% const currentPage = typeof page !== 'undefined' ? page : 1; %>
<% if(passages.length == 0 && currentPage == 1){ %>
    <div style="margin:auto;text-align:center;">No Passages Yet.</div>
<% } %>
